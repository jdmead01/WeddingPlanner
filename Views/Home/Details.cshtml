<!-- Make sure to import the models here  -->
@model WeddingPlanner.Models.Wedding


<div class="wrapper">
    <div class="container">
        <div class="header"></div>
        <div class="body">
                <form asp-action="Create" asp-controller="Home" method="post">
                        <div class="form-group">
                            <label for="name">Wedder One:</label>
                            <input type="text" asp-for="wedderOne" class="form-control"
                                placeholder="Enter your first name">
                        </div>
                        <div class="form-group">
                                <label for="name">Wedder Two:</label>
                                <input type="text" asp-for="wedderTwo" class="form-control"
                                    placeholder="Enter your first name">
                            </div>
                       
                        <div class="button">
                            <button class="btn btn-primary" type="submit">Register</button>
                        </div>
                    </form>
        </div>
    </div>
</div>